{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch",
      "sourceMaps": true,
      "stopOnEntry": true,
      "console": "internalConsole",
      "cwd": "${workspaceRoot}",
      "program": "${workspaceRoot}/app/index.ts",
      "outFiles": [
        "${workspaceRoot}/src/*.js"
      ]
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach to Process",
      "port": 5858,
      "outFiles": []
    }
  ]
}
