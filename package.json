    "init-npm": "npm link mobx mobx-react js-plus tslib ts-jest jest @types/jest @types/node",
    "init-cra-base": "npm link react@15.4.1 react-dom@15.4.1 react-scripts @types/react @types/react-dom",
    "init-crna-base": "npm link expo@15.1.3 react@15.4.1 react-native@0.42.3 react-native-scripts@0.0.27 react-test-renderer@15.4.1 @types/react-native jest-expo@0.3.0",
    "install-eslint": "npm install -g eslint-config-react-app eslint babel-eslint eslint-plugin-react eslint-plugin-import eslint-plugin-jsx-a11y eslint-plugin-flowtype eslint-plugin-import",
    "init-cra": "run-s init-cra init-npm",
    "init-crna": "run-s init-crna init-npm",
    "prettier": "prettier --write \"app/**.*(ts|tsx)\" --parser typescript --single-quote --bracket-spacing=false --no-semi"
  },
  "devDependencies": {
    "react-scripts": "",
    "tslib": "",
    "@types/react": "",
    "@types/react-dom": "",
